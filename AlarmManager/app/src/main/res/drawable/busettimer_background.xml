<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient android:startColor="#c4d4ed" android:endColor="#8da2c4"/>
            <corners android:radius="25dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#bec1c6" android:endColor="#424344"/>
            <corners android:radius="25dp" />
        </shape>

    </item>
</selector>